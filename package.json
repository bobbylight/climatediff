{
  "name": "climatediff",
  "version": "0.4.0",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist coverage",
    "build": "cross-env NODE_ENV=production node_modules/.bin/webpack --config webpack/webpack.config.js",
    "watch": "cross-env NODE_ENV=dev webpack --config webpack/webpack.config.js --watch",
    "livereload": "livereload dist",
    "test": "cross-env NODE_ENV=test karma start karma.unit.js",
    "coverage": "cross-env NODE_ENV=test karma start karma.coverage.js",
    "webpack-analyze": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.js --json > size-info.json",
    "webpack-size-view": "webpack-bundle-size-analyzer size-info.json > size-info.txt"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bobbylight/climatediff.git"
  },
  "devDependencies": {
    "@types/bootstrap": "3.3.36",
    "@types/d3": "4.11.1",
    "@types/jquery": "3.2.16",
    "copy-webpack-plugin": "4.2.1",
    "cross-env": "5.1.1",
    "css-loader": "0.28.7",
    "del": "3.0.0",
    "file-loader": "1.1.5",
    "html-webpack-plugin": "2.30.1",
    "less": "2.7.3",
    "less-loader": "4.0.5",
    "livereload": "0.6.3",
    "raw-loader": "0.5.1",
    "rimraf": "2.6.2",
    "run-sequence": "2.2.0",
    "style-loader": "0.19.0",
    "ts-loader": "3.1.1",
    "tslint": "5.8.0",
    "tslint-loader": "3.5.3",
    "typescript": "2.6.1",
    "uglifyjs-webpack-plugin": "1.1.2",
    "url-loader": "0.6.2",
    "vue-loader": "13.5.0",
    "vue-template-compiler": "2.5.9",
    "webpack": "3.8.1",
    "webpack-bundle-size-analyzer": "2.7.0"
  },
  "dependencies": {
    "bootstrap": "3.3.7",
    "d3": "4.12.0",
    "font-awesome": "4.7.0",
    "jquery": "3.2.1",
    "vue": "2.5.9",
    "vue-js-toggle-button": "1.2.0",
    "vue-router": "3.0.1",
    "vue-simple-spinner": "1.2.8",
    "vuetify": "0.17.4"
  }
}
