    "@vitejs/plugin-vue": "5.0.4",
    "eslint": "8.57.0",
    "eslint-plugin-vue": "9.24.1",
    "rimraf": "2.6.2",
    "typescript": "5.2.2",
    "vite": "5.2.8",
    "vite-plugin-vuetify": "2.0.3"
  },
  "dependencies": {
    "d3": "5.7.0",
    "font-awesome": "4.7.0",
