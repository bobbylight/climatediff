{
  "name": "climatediff",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "clean": "rm -fr dist coverage",
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .ts,.vue --ignore-path .gitignore --fix src",
    "make-aws-archive": "rm -f aws-archive.zip && cd dist && zip -r aws-archive.zip . && mv ./aws-archive.zip ../"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bobbylight/climatediff.git"
  },
  "devDependencies": {
    "@mdi/font": "7.4.47",
    "@stylistic/eslint-plugin": "1.7.2",
    "@types/d3": "7.4.3",
    "@typescript-eslint/parser": "7.12.0",
    "@vitejs/plugin-vue": "5.0.5",
    "eslint": "8.57.0",
    "eslint-plugin-vue": "9.26.0",
    "typescript": "5.4.5",
    "vite": "5.2.13",
    "vite-plugin-eslint": "1.8.1",
    "vite-plugin-vuetify": "2.0.3",
    "vue-tsc": "2.0.21"
  },
  "dependencies": {
    "d3": "7.9.0",
    "vue": "3.4.27",
    "vue-router": "4.3.2",
    "vuetify": "3.6.8"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.18.0"
  }
}
