{
  "name": "climatediff",
  "version": "0.8.0",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist coverage",
    "build": "cross-env NODE_ENV=production node_modules/.bin/webpack --config webpack/webpack.config.js",
    "watch": "cross-env NODE_ENV=dev webpack --config webpack/webpack.config.js --watch",
    "livereload": "livereload dist",
    "test": "cross-env NODE_ENV=test karma start karma.unit.js",
    "coverage": "cross-env NODE_ENV=test karma start karma.coverage.js",
    "make-aws-archive": "rm -f aws-archive.zip && cd dist && zip -r aws-archive.zip . && mv ./aws-archive.zip ../",
    "webpack-analyze": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.js --json > size-info.json",
    "webpack-size-view": "webpack-bundle-size-analyzer size-info.json > size-info.txt"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bobbylight/climatediff.git"
  },
  "devDependencies": {
    "@types/d3": "4.12.0",
    "copy-webpack-plugin": "4.3.1",
    "cross-env": "5.1.3",
    "css-loader": "0.28.7",
    "del": "3.0.0",
    "file-loader": "1.1.6",
    "html-webpack-plugin": "2.30.1",
    "less": "2.7.3",
    "less-loader": "4.0.5",
    "livereload": "0.6.3",
    "raw-loader": "0.5.1",
    "rimraf": "2.6.2",
    "run-sequence": "2.2.0",
    "style-loader": "0.19.1",
    "stylus": "0.54.5",
    "stylus-loader": "3.0.1",
    "ts-loader": "3.2.0",
    "tslint": "5.8.0",
    "tslint-loader": "3.5.3",
    "typescript": "2.6.2",
    "uglifyjs-webpack-plugin": "1.1.5",
    "url-loader": "0.6.2",
    "vue-class-component": "6.1.2",
    "vue-loader": "13.6.2",
    "vue-property-decorator": "6.0.0",
    "vue-template-compiler": "2.5.13",
    "webpack": "3.10.0",
    "webpack-auto-inject-version": "1.0.0",
    "webpack-bundle-size-analyzer": "2.7.0"
  },
  "dependencies": {
    "d3": "4.12.2",
    "font-awesome": "4.7.0",
    "vue": "2.5.13",
    "vue-js-toggle-button": "1.2.2",
    "vue-router": "3.0.1",
    "vuetify": "0.17.6"
  }
}
