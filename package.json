{
  "name": "climatediff",
  "version": "0.8.1",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist coverage",
    "build": "cross-env NODE_ENV=production node_modules/.bin/webpack --config webpack/webpack.config.js",
    "watch": "cross-env NODE_ENV=dev webpack --config webpack/webpack.config.js --watch",
    "livereload": "livereload dist",
    "test": "cross-env NODE_ENV=test karma start karma.unit.js",
    "coverage": "cross-env NODE_ENV=test karma start karma.coverage.js",
    "make-aws-archive": "rm -f aws-archive.zip && cd dist && zip -r aws-archive.zip . && mv ./aws-archive.zip ../",
    "webpack-analyze": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.js --json > size-info.json",
    "webpack-size-view": "webpack-bundle-size-analyzer size-info.json > size-info.txt"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bobbylight/climatediff.git"
  },
  "devDependencies": {
    "@types/d3": "4.12.0",
    "copy-webpack-plugin": "4.5.2",
    "cross-env": "5.2.0",
    "css-loader": "1.0.0",
    "del": "3.0.0",
    "file-loader": "2.0.0",
    "html-webpack-plugin": "3.2.0",
    "less": "3.8.1",
    "less-loader": "4.1.0",
    "livereload": "0.7.0",
    "raw-loader": "0.5.1",
    "rimraf": "2.6.2",
    "run-sequence": "2.2.1",
    "style-loader": "0.23.0",
    "stylus": "0.54.5",
    "stylus-loader": "3.0.2",
    "ts-loader": "5.2.1",
    "tslint": "5.11.0",
    "tslint-loader": "3.6.0",
    "typescript": "3.1.1",
    "uglifyjs-webpack-plugin": "2.0.1",
    "url-loader": "1.1.1",
    "vue-class-component": "6.2.0",
    "vue-loader": "15.4.2",
    "vue-property-decorator": "7.1.1",
    "vue-template-compiler": "2.5.17",
    "webpack": "4.20.2",
    "webpack-auto-inject-version": "1.1.0",
    "webpack-bundle-size-analyzer": "2.7.0",
    "webpack-cli": "3.1.2"
  },
  "dependencies": {
    "d3": "4.13.0",
    "font-awesome": "4.7.0",
    "vue": "2.5.17",
    "vue-js-toggle-button": "1.3.0",
    "vue-router": "3.0.1",
    "vuetify": "1.2.5"
  }
}
